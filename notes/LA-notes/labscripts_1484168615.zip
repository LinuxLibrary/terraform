        font-family: 'Helvetica', Arial, sans-serif;
        font-weight: 800;
        display: block;
    }
    .callout.red-hat .red-hat-text ul {
        list-style: none;
        margin: 5px 0 0;
        padding: 0;
    }
    .callout.red-hat .red-hat-text ul li {
        text-transform: uppercase;
        font-weight: 100;
        color: #fff;
        line-height: 1;
        font-size: 16px;
    }
    /* google cloud platform ad styles */
    .callout.google-cloud-platform {
        min-height: 50vh;
        background: url('/templates/cp/assets/img/login/Google-Cloud-Platform.jpg') 0 0 no-repeat;
        background-size: cover;
