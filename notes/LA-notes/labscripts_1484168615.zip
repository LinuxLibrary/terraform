    }
    .callout.azure .azure-logo svg {
        display: block;
        width: 50%;
        height: auto;
        float: right;
    }
    /* Orion Ad Styles */
    .callout.orion {
        min-height: 50vh;
        background: url('//assets.linuxacademy.com/templates/cp/assets/img/login/paper-bg.jpg') 0 0 no-repeat;
        background-size: cover;
    }
    .callout.orion .orion {
        display: none;
        margin: 0 auto;
    }
    .callout.orion .orion img {
        display: block;
        width: 100%;
