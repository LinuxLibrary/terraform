    }
    .callout.orion .orion-text { color: #fff; text-align: center; }
    .callout.orion .orion-text h2 {
        font-weight: 100;
        text-transform: uppercase;
        letter-spacing: 5px;
        font-size: 30px;
        color: #fff;
    }
    .callout.orion .orion-text h3 {
        font-size: 16px;
        font-weight: 600;
        letter-spacing: 15px;
        text-transform: uppercase;
        font-family: 'Helvetica', Arial, sans-serif;
        color: #fff;
        margin-bottom: 0;
    }
    .callout.orion .orion-text p {
        font-size: 14px;
        margin-top: 10px;
        text-transform: uppercase;
        letter-spacing: 10px;
        font-weight: 100;
        padding: 0 1%;
        line-height: 2;
    }
    .callout.orion .btn-outline {
        display: inline-block;
        border: 3px solid #ec5951;
        color: #ec5951;
        text-decoration: none;
        text-transform: uppercase;
        padding: 15px;
        font-weight: 600;
        transition: all ease 0.5s;
        border-radius: 5px;
    }
    .callout.orion .btn-outline:hover {
        background: #ec5951;
        color: #fff;
    }
    /* Red Hat Ad Styles*/
    .callout.red-hat {
        min-height: 50vh;
        background: url('/templates/cp/assets/img/login/red-hat.png') 0 0 no-repeat;
        background-size: cover;
    }
    .callout.red-hat .red-hat-text {
        max-width: 70%;
    }
    .callout.red-hat .red-hat-text h2,
    .callout.google-cloud-platform .google-cloud-platform-text h2,
    .callout.promo-ad .promo-ad-text h2 {
        color: #fff;
        font-weight: 100;
        text-transform: uppercase;
        margin: 0;
        padding: 0;
        font-size: 18px;
        line-height: 1;
    }
    .callout.google-cloud-platform .google-cloud-platform-text h2 {
        font-size: 22px;
    }
    .callout.red-hat .red-hat-text h2 strong,
    .callout.google-cloud-platform .google-cloud-platform-text h2 strong,
