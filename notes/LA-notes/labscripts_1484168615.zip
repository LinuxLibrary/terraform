    .callout.google-cloud-platform .google-cloud-platform-text {
        max-width: 70%;
    }
    .callout.google-cloud-platform .google-cloud-platform-text h2 strong {
        color:#231f20;
    }
    /*Promo Ad Styles*/
    .callout.promo-ad {
        min-height: 76vh;
        background: url('/templates/cp/assets/img/login/Promo-Ad-Background.png') 0 0 no-repeat;
        background-size: cover;
    }
    .callout.promo-ad .promo-ad-text {
        position: relative;
        z-index: 1;
    }
    .callout.promo-ad .promo-ad-text h2 {
        font-size: 36px;
        margin-bottom: 6px;
    }
    .callout.promo-ad .promo-ad-text h3 {
        color:White;
        font-size: 32px;
        font-weight: 200;
        margin-bottom: 4px;
    }
    .callout.promo-ad .promo-ad-text h3 strong {
        font-family: 'Helvetica', Arial, sans-serif;
        font-weight: 800;
        margin-left: 12px;
    }
    .callout.promo-ad .promo-ad-text h3 strong span {
        font-size: 16px;
        color:White;
    }
    .callout.promo-ad .promo-ad-text span {
        color:#b5d2ed;
        font-size: 26px;
    }
    .callout.promo-ad .promo-ad-text span.sub-title {
        display: block;
        margin-bottom: 40px;
        font-weight: 200;
    }
    .callout.promo-ad .promo-ad-text ul {
        list-style-type: none;
        color:#b5d2ed;
        -webkit-padding-start: 0;
        margin-bottom: 30px;
    }
    .promo-footer-text span:first-child {
        color:White;
        font-weight: 400;
        display: block;
    }
    .promo-footer-text span:nth-child(2) {
        color:#b5d2ed;
        font-weight: 400;
    }
    @-webkit-keyframes animateCloud {
        0% {margin-left: -400px;}
        100% {margin-left: 100%;}
    }
    @-moz-keyframes animateCloud {
        0% {margin-left: -400px;}
        100% {margin-left: 100%;}
    }
    @keyframes animateCloud {
        0% {margin-left: -400px;}
        100% {margin-left: 100%;}
    }
    .x1, .x2, .x3 {
        position: absolute;
        z-index: 0;
    }
    .x1 {
        top: 200px;
        margin-left: -400px;
        -webkit-animation: animateCloud 90s linear infinite;
        -moz-animation: animateCloud 90s linear infinite;
        animation: animateCloud 90s linear infinite;
        -webkit-animation-delay: 16s;
        animation-delay: 16s;
        
        -webkit-transform: scale(0.65);
        -moz-transform: scale(0.65);
        transform: scale(0.65);
    }
    .x2 {
        top: 20px;
        left: -100px;
        -webkit-animation: animateClou 60s linear infinite;
        -moz-animation: animateCloud 60s linear infinite;
        animation: animateCloud 60s linear infinite;
        
        -webkit-transform: scale(0.3);
        -moz-transform: scale(0.3);
        transform: scale(0.3);
    }
    .x3 {
        top: 370px;
        left: 0;
        -webkit-animation: animateCloud 40s linear infinite;
        -moz-animation: animateCloud 40s linear infinite;
        animation: animateCloud 40s linear infinite;
        
        -webkit-transform: scale(0.5);
        -moz-transform: scale(0.5);
        transform: scale(0.5);
    }
    .cloud {
        background: #597896;
        background: linear-gradient(top,  #fff 5%,#f1f1f1 100%);
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fff', endColorstr='#f1f1f1',GradientType=0 );
        
        border-radius: 100px;
        box-shadow: 0 8px 5px rgba(0, 0, 0, 0.1);
        height: 120px;
        position: relative;
        width: 350px;
    }
    .cloud:after, .cloud:before {
        background: #597896;
        content: '';
        position: absolute;
        z-index: -1;
    }
    .cloud:after {
        border-radius: 100px;
        height: 100px;
        left: 50px;
        top: -50px;
        width: 100px;
    }
    .cloud:before {
        border-radius: 200px;
        width: 180px;
        height: 180px;
        right: 50px;
        top: -90px;
    }
    @media (min-width: 576px) {
        .callout.google-cloud-platform .google-cloud-platform-text h2,
        .callout.red-hat .red-hat-text h2 {
            font-size:26px;
        }

            .logo-container {
                max-width: 50%;
            }

            .callout.promo-ad {
                min-height: 68vh;
            }
        }

        @media (min-width: 768px) {
            .callout.google-cloud-platform .google-cloud-platform-text h2,
            .callout.red-hat .red-hat-text h2 {
                font-size: 28px;
            }

            .logo-container {
                max-width: 40%;
            }
        }

        @media (min-width: 992px) {
            .login.section {
                min-height: 100vh;
            }

            .callout, .callout.azure, .callout.orion {
                min-height: 100vh;
            }

            .callout.azure h3 {
                font-size: 42px;
            }

            .callout.orion .orion {
                display: block;
                max-width: 60%
            }

            .callout.orion .orion-text h2 {
                font-size: 60px;
            }

            .callout.orion .orion-text h3 {
                font-size: 32px;
            }

            .callout.orion .orion-text p {
                font-size: 18px;
                margin-top: 40px;
                padding: 0 25%;
            }

            .callout.red-hat,
            .callout.google-cloud-platform,
            .callout.promo-ad {
                min-height: 100vh;
            }

            .callout.red-hat .red-hat-text {
                position: absolute;
                bottom: 5%;
                left: 5%;
            }

            .callout.google-cloud-platform .google-cloud-platform-text {
                position: absolute;
                top: 4%;
                left: 8%;
            }

            .callout.red-hat .red-hat-text h2,
            .callout.google-cloud-platform .google-cloud-platform-text h2 {
                font-size: 32px;
            }

            .callout.red-hat .red-hat-text ul li {
                font-size: 24px;
            }

            .logo-container {
                max-width: 60%;
                margin-top: 15%;
            }

            .login.section h2 {
                margin-top: 60px;
            }

            .login.section {
                padding: 50px 30px 50px 0;
            }

            .login.section .login-form-container {
                max-width: 65%;
            }

            .callout.promo-ad .promo-ad-text span.sub-title {
                max-width: 65%;
            }
        }
    </style>
